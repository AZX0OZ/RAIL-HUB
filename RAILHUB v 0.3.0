    Increment = 0.1,
    ValueName = "Скорость",
    Callback = function(Value)
        getgenv().TpWalking = true
        getgenv().Speed = Value

        local player = game:GetService("Players").LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait() -- Wait for the character to be available
        local humanoid = character:FindFirstChildWhichIsA("Humanoid")
        
        local RunService = game:GetService("RunService")
        
        -- Ensure the loop only runs if humanoid exists
        if humanoid then
            RunService.Heartbeat:Connect(function()
                if getgenv().TpWalking and character and humanoid and humanoid.Parent then
                    -- If the player is moving, apply speed
                    if humanoid.MoveDirection.Magnitude > Value then
                        character:TranslateBy(humanoid.MoveDirection * getgenv().Speed * RunService.Heartbeat:Wait() * Value)
                    end
                end
            end)
        else
            warn("Humanoid not found!")
        end 
    end  
})



