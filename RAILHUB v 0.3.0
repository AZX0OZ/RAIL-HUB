	Name = "Бесконечные прыжки",
	Default = false,
	Callback = function(Value)
local Player = game:GetService('Players').LocalPlayer
local UIS = game:GetService('UserInputService')

-- Variable to control jump height functionality
_G.JumpHeight = 50
_G.IsJumpEnabled = Value -- This variable acts as a toggle

function Action(Object, Function)
    if Object ~= nil then
        Function(Object)
    end
end

-- Function to toggle jump functionality
function ToggleJump()
    _G.IsJumpEnabled = not _G.IsJumpEnabled
    print("Jump functionality is now: " .. tostring(_G.IsJumpEnabled))
end

-- Connect the space key to the jump increase
UIS.InputBegan:connect(function(UserInput)
    if UserInput.UserInputType == Enum.UserInputType.Keyboard then
        -- Check if the Space key is pressed
        if UserInput.KeyCode == Enum.KeyCode.Space then
            Action(Player.Character.Humanoid, function(self)
                -- Only apply jump height if the toggle is enabled
                if _G.IsJumpEnabled and (self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall) then
                    Action(self.Parent.HumanoidRootPart, function(rootPart)
                        rootPart.Velocity = Vector3.new(0, _G.JumpHeight, 0)
                    end)
                end
            end)
        end
        
        -- Check if the 'T' key is pressed to toggle jump functionality
        if UserInput.KeyCode == Enum.KeyCode.T then
            ToggleJump()
        end
    end
end)
	end    
})










